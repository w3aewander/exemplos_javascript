/*****************************************************************************
 *
 *  Trocando cédula
 *
 *****************************************************************************/
var arr_troco = [2,5,10,20,25,50,100];
int i;

for ( i in arr_troco){
   if ( 50 % arr_troco[i] === 0 ){
       console.log( "Cédulas de " + arr_troco[i] + ": " + 50 / arr_troco[i] ); 
   }
}

/* Fim */

